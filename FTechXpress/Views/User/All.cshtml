
@model List<UserViewModel>

	<h1> All Users </h1>

<table class="table table-striped" >
	<thead>
		<tr>
		
			<th> Name</th>
			<th>Email</th>
			<th>Phone Number</th>
			<th>Address</th>
			<th>Function</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var user in Model)
		{
			<tr >

				<td style="background:@user.Color">@user.Name</td>
				<td>@user.Email</td>
				<td>@user.PhoneNumber</td>
				<td>@user.Address</td>
				<td><a type="button" class= "btn btn-info" href="/User/Details/@user.Id"> Details </a>
					
				</td>
			</tr>
		}
	</tbody>
